<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title> Test Results</title>
<style>
code
{
white-space: pre;
}
.test-result
{
padding-left: 30px;
background-color: green;
}
.red
{
background-color: #c00;
}
.green
{
background-color: #6c6;
}
.level1
{
 padding-left: 10px;
}
.level2
{
 padding-left: 30px;
}

#suite
{
width: 80%;
border-collapse: collapse;
border: 1px solid #777;
table-layout:fixed;
}
#suite th
{
font-weight: bold;
color: white;
background-color: #777;
}

#suite td {
border: 1px solid #777;
padding: 2px;
word-wrap:break-word;
}
.case
{
border-collapse:collapse;
}
.failCase
{
font-weight:bold;
color:#c00;
}
.passCase
{
font-weight:bold;
color:#106A10;
}
.cage
{
background-color: #E6E6D6;
text-align: left;
font-size: 8pt;
width: 300px;
}
</style>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script type="text/javascript">
function open_suite_details(ele)
{
var target = $(ele).parent().parent().next()[0]
var displayState = target.style.display;
if(displayState == 'none')
{
  target.style.display = 'block';
}
else
{
 target.style.display = 'none';
}
}

function show_fail_details(ele)
{
var target = $(ele).next()[0]
var displayState = target.style.display;
if(displayState == 'none')
{
  target.style.display = 'block';
  target.style.fontSize = '50%';
}
else
{
 target.style.display = 'none';
}
}
</script>
</head>
<body>
